{
  "name": "quizserver",
  "version": "1.0.0",
  "description": "Delicious-Shop",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "concurrently \"node ./server/index.js\" \"cd react && npm start\"",
    "dev": "concurrently \"nodemon ./server/index.js\" \"cd react && npm start\"",
    "heroku-postbuild": "cd react/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react/node_modules"
  ],
  "dependencies": {
    "cloudinary": "^1.11.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "mongoose": "^5.3.14",
    "multer": "^1.3.1",
    "multer-storage-cloudinary": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "heroku",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  }
}
